{
  "name": "cleverclub",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:quiz": "npm --prefix QuizDashboard run dev",
    "dev:both": "concurrently \"npm run dev\" \"npm run dev:quiz\"",
    "build": "vite build",
    "build:quiz": "npm --prefix QuizDashboard run build",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview:quiz": "npm --prefix QuizDashboard run preview",
    "install:all": "npm install && npm --prefix QuizDashboard install"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "vite": "^7.1.2"
  }
}
